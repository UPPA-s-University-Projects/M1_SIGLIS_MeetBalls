	<persistence-unit name="MeetBalls">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>fr.pau.univ.meetballs.model.CookType</class>
		<class>fr.pau.univ.meetballs.model.Diet</class>
		<class>fr.pau.univ.meetballs.model.Dish</class>
		<class>fr.pau.univ.meetballs.model.User</class>
		<class>fr.pau.univ.meetballs.model.Discussion</class>
		<class>fr.pau.univ.meetballs.model.Msg</class>
		<class>fr.pau.univ.meetballs.model.Restaurant</class>
